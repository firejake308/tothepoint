{
    "manifest_version": 3,
    "name": "To the Point",
    "version": "1",
    "content_scripts": [{
        "matches": ["*://*.uptodate.com/*"],
        "js": ["index.js"]
    }],
    "web_accessible_resources": [
        {
            "resources": ["tf2.min.js", "qna.js", "atq.js", "biobert_js/*"],
            "matches": ["*://*.uptodate.com/*"]
        }
    ]
}